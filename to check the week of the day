#24 to check the week of the day
a=int(input("enter a number:"))
if(a==1):
        print("sunday")
elif(a==2):
        print("monday")
elif(a==3):
        print("tuesday")
elif(a==4):
        print("wednesday")
elif(a==5):
        print("thursday")
elif(a==6):
        print("friday")
elif(a==7):
        print("sunday")
else:
        print("invalid")
